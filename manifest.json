{
  "name": "A browser action with a popup that changes the page color.",
  "background_page": "background.html",
  "version": "1.0",
  "content_scripts":[{
  "js":["script.js"],
  "matches":["*://*.youtube.com/*"]
  }],
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "icon.png",
      "popup": "popup.html"
  }
}